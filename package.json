		"webpack": "^5.75.0",
		"webpack-cli": "^4.9.2",
		"eslint": "^8.28.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"jest": "^28.1.3",
		"@babel/preset-env": "^7.17.10",
		"@babel/preset-react": "^7.16.7",
