	"description": "Multi pod and container logs for Lens",
	"publisher": "@andrea-falco/lens-multi-pod-logs",
	"main": "dist/main.js",
	"renderer": "dist/renderer.js",
	"engines": {
		"node": ">=16 <17",
		"lens": "^6.0.0"
	},
	"keywords": [
		"lens",
